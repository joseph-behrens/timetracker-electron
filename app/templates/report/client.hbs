<h3>Filtered by Client {{model.[0].Client}}</h3>

<table>
    <tr>
        <th>Client</th>
        <th>Project</th>
        <th>Description</th>
        <th>Start</th>
        <th>End</th>
    </tr>
    {{#each model as |item|}}
        <tr>
            <td>{{item.Client}}</td>
            <td>{{item.Project}}</td>
            <td>{{item.Description}}</td>
            <td>{{item.StartTime}}</td>
            <td>{{item.EndTime}}</td>
        </tr>
    {{/each}}
</table>