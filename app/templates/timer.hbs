    <div>
        <label for="client"><strong>Client: </strong></label>
        <input type="text" id="client" placeholder="{{model.[0].Client}}">
    </div>

    <div>
        <label for="project"><strong>Project: </strong></label>
        <input type="text" id="project" placeholder="{{model.[0].Project}}">
    </div>

    <div>
        <label for="description"><strong>Description: </strong></label>
        <input type="text" id="description" placeholder="{{model.[0].Description}}">
    </div>

    <div>
        <label for="start"><strong>Start: </strong></label>
        <input type="datetime" id="start" placeholder="{{model.[0].StartTime}}">
